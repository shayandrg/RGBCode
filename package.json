{
    "name": "rgb_code",
    "displayName": "RGBCode",
    "description": "make your vscode RGB",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.74.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:RGBCode.active",
        "onCommand:RGBCode.deactive",
        "onStartupFinished"
    ],
    "main": "./src/main.js",
    "contributes": {
        "commands": [
            {
                "command": "RGBCode.active",
                "title": "Active / Refresh",
                "category": "RGBCode"
            },
            {
                "command": "RGBCode.deactive",
                "title": "Deactive",
                "category": "RGBCode"
            }
        ],
        "configuration": {
            "title": "RGB Code",
            "properties": {
                "rgbCode.palette": {
                    "type": "string",
                    "default": "Sunset",
                    "enum": [ "Sunset", "Instagram" ],
                    "description": "Your favorite color plate for RGB animation"
                },
                "rgbCode.duration": {
                    "type": "number",
                    "default": 3,
                    "enum": [ 1, 1.5, 2, 2.5, 3, 3.5, 4, 4.5, 5, 5.5, 6, 6.5, 7, 7.5, 8, 8.5, 9, 9.5, 10 ],
                    "description": "Css animation duration (Seconds)"
                },
                "rgbCode.height": {
                    "type": "number",
                    "default": 2,
                    "enum": [ 1, 1.5, 1.75, 2, 2.5, 3, 3.5, 4, 4.5, 5 ],
                    "description": "Slider height (px)"
                }
            }
        }
    },
    "scripts": {
    },
    "devDependencies": {
        "@types/glob": "^8.0.0",
        "@types/mocha": "^10.0.1",
        "@types/node": "16.x",
        "@types/vscode": "^1.74.0",
        "@vscode/test-electron": "^2.2.0",
        "eslint": "^8.28.0",
        "glob": "^8.0.3",
        "mocha": "^10.1.0",
        "typescript": "^4.9.3"
    },
    "dependencies": {
        "jsdom": "^20.0.3"
    }
}